const express = require('express');

const app = express();

const port = 5004; // Porta em que o servidor ser치 executado

// Rota para exibir a p치gina index.html

app.get('/', (req, res) => {

res.sendFile(__dirname + '/paginamedico.html');



});

app.get('/paginamedico.css', (req, res) => {

res.sendFile(__dirname + '/paginamedico.css');



});

// Rota para exibir a p치gina outra_pagina.html

app.get('/outra_pagina', (req, res) => {

res.sendFile(__dirname + '/outra_pagina.html');

});

// Iniciar o servidor

app.listen(port, () => {

console.log(`Servidor Express est치 rodando na porta ${port}`);

});

